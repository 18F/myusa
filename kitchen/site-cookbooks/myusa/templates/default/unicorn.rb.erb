# Set the working application directory
working_directory "<%= @working_dir %>"

# Unicorn PID file location
pid "<%= @pids_dir %>/unicorn.pid"

# Path to logs
stderr_path "<%= @log_dir %>/unicorn.log"
stdout_path "<%= @log_dir %>/unicorn.log"

# Unicorn socket
listen "/tmp/unicorn.<%= @app_id %>.sock"

# Number of processes
# worker_processes 4
worker_processes 2

# Time-out
timeout 30
